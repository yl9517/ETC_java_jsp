package msa08;

//bank, creditline 실행 클래스

public class OverrideTest2 {

	public static void main(String[] args) {
		
		Bank b1 = new Bank("111", "hong", 100, 0);
		b1.deposit(100);
		b1.print();
		
		b1.deposit(50);
		b1.print();
		
		b1.withdraw(400); //입력오류
		
		b1.withdraw(130);
		b1.print();
		
		System.out.println("------------");

		Bank_CreditLine c1 = new Bank_CreditLine("2222", "lee", 100, 1000); //-1000원 까지만 
		
		c1.deposit(400);
		
		c1.withdraw(600);
		c1.print();
		c1.withdraw(1000);
		
		
	}

}
