package msa05;
/*
 * 이차원배열
 * 
 */
public class ArrTest4 {

	public static void main(String[] args) {
		
		int[][] arr = new int[3][4]; //행 수(세로줄갯수(가로줄들)),열 수(가로줄갯수(세로줄들))
//		0,0  0,1  0,2  0,3 	← 0번째 사람에 대한 정보들
//		1,0  1,1  1,2  1,3
//		2,0  2,1  2,2  2,3
		
//		for(int i=0; i<3; i++) { 
//			for(int j=0; j<4; j++) {
//				System.out.print(arr[i][j]+"  ");
//			}
//			System.out.println();
//		}
		
		System.out.println(arr.length);  //3 (arr의 세로길이 - 몇명인지)
		System.out.println(arr[0].length);  //4 ← 0번째 줄의 길이
		System.out.println(arr[1].length);  //4
		System.out.println(arr[2].length);  //4
		
		
	}

}
