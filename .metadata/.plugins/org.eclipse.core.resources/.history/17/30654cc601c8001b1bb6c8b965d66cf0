package msa07;

public class Man {
	private String name;
	private int age;
	private String addr;
	private String phone;
	
	//name,age 변경 메소드
	public void	modify(String name, int age){
		this.name=name;
		this.age=age;
	}
	//name,addr 변경 메소드
	public void	modify(String name, String addr){ //메소드 오버로딩
		this.name=name;
		this.addr=addr;
	}
	
	public void print() {
		System.out.print(name+" "+age+" "+addr);
	}
	
	
	
	
	
}


