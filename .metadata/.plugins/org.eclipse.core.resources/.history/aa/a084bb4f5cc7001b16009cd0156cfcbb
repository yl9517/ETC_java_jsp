package msa06;
/*
 * 캡슐화 (private)
 * 
 */
class Access1{
	
	private int su;
	
	//생성자
	Access1(){
		
	} 
	
	Access1(int su){
		if(su<0)
			su=0;
		this.su=su;
	}
	
	//메소드
	void prt() {
		System.out.println("Su : "+su);
	}
}
public class AccessTest {
	public static void main(String[] args) {
		Access1 ins = new Access1();
//		ins.su = -100;    >>   priavte 필드라서 접근 못함!!!
//		System.out.println(ins.su);

		
		Access1 ins2 = new Access1(23);
		
	}
}
