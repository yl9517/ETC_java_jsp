package msa06;
/*
 *  
 *  [생성자] 객체를 만드는 자 
 *     ┗ 클래스 이름과 같은 이름!! 리턴타입 없음!! 
 * 
 */

class Student{
	//필드
	String name;
	
	//생성자
	Student(String n){
		name=n;
	}
}
public class ConstructorTest{

	public static void main(String[] args) {
		Student s1 = new Student("hong gil dong");
		System.out.println(s1.name);
	}
}
