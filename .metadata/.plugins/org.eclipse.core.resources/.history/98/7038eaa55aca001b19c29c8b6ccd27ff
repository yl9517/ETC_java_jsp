package msa10;

public class SuperSubTest1 {

	public static void main(String[] args) {

		Super1 ins = new Sub1(); //up Casting
		ins.prt();
		ins.view();
		 
//		Sub1 ins2 = (Sub1) new Super1(); //ClassCastException 클래스 변환 오류
		
		Super1 ins3 = new Sub1();
		
		Sub1 ins4 = (Sub1) ins3;
		
		ins4.draw();
		ins4.prt();
		ins4.view();
		
		ins3.prt();
		ins3.view();
		
	}

}
