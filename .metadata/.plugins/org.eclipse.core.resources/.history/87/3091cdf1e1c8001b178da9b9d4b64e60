package msa08;

// super(); = 부모클래스의 생성자 호출

class Super5{
	//필드
	public String name;
	//생성자
	public Super5(String name) {
		this.name=name;
	}
	
}

class Sub5 extends Super5{ //여러개의 부모클래스를 상속받을 수 X
	
	public Sub4() {
		super(); //부모클래스의 생성자 호출 (Super4())
		System.out.println("Sub4 Constructor");
	}
	
	
}

public class SuperSubTest5 {

	public static void main(String[] args) {
		Super4 ins = new Super4(); //클래스의 생성자 이용하여 인스턴스화
		Sub4 ins2 = new Sub4();
		
		
		
	}

}
